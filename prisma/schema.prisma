datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Package {
  id            String    @id @default(cuid())
  name          String
  contact       String?
  description   String?
  location      String
  cost          Float
  duration      String?
  image         String
  activities    String[]
  details_file  String?
  updated_at    String?
  video_link    String?
  is_premium_flag Boolean?
}
